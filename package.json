{
	"name": "uelements",
	"description": "uelements provides a functional way of defining custom elements",
	"version": "0.3.0",
	"source": "./src/index.ts",
	"main": "./dist/uelements.umd.js",
	"module": "./dist/uelements.es.js",
	"unpkg": "./dist/uelements.iife.js",
	"types": "./index.d.ts",
	"files": [
		"dist/",
		"example/",
		"*.d.ts",
		"README.md"
	],
	"scripts": {
		"dev": "vite",
		"build": "node vite.build.js",
		"serve": "vite preview"
	},
	"exports": {
		".": {
			"types": "./src/index.d.ts",
			"browser": "./dist/uelements.es.js",
			"umd": "./dist/uelements.umd.js",
			"import": "./dist/uelements.es.js",
			"require": "./dist/uelements.umd.js",
			"default": "./dist/uelements.cjs.js"
		},
		"./sinuous": {
			"types": "./src/sinuous/index.d.ts",
			"browser": "./dist/sinuous.es.js",
			"umd": "./dist/sinuous.umd.js",
			"import": "./dist/sinuous.es.js",
			"require": "./dist/sinuous.umd.js",
			"default": "./dist/sinuous.cjs.js"
		}
	},
	"devDependencies": {
		"@types/node": "^17.0.23",
		"typescript": "^4.5.4",
		"vite": "^2.8.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kethan/uelements.git"
	},
	"keywords": [
		"functional",
		"reactive",
		"declarative",
		"dom",
		"custom",
		"elements",
		"define",
		"lifecycle",
		"web components",
		"components",
		"custom element",
		"hooks",
		"react",
		"preact"
	],
	"author": "Kethan Surana",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/kethan/uelements/issues"
	},
	"homepage": "https://github.com/kethan/uelements#readme",
	"dependencies": {
		"preact": "^10.7.1",
		"preact-lazy": "^0.0.3",
		"sinuous": "^0.28.1",
		"uwhen": "^0.2.2"
	}
}
