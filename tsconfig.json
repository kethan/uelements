{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    // "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "noImplicitAny": false
  },
  "include": [
    "src",
    "src/sinuous/index.d.ts",
    "src/uelements/index.d.ts",
    "index.d.ts"
  ],
  "paths": {
    "sinuous": [
      "./src/sinuous/index"
    ],
    "uelements": [
      "./src/uelements/index"
    ]
  }
}